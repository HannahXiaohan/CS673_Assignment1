def print_multiplication_table():
    size = 12
    print("   ", end=" ")
    for i in range(1, size + 1):
        print(f"{i:4}", end=" ")
    print("\n" + "-" * (5 * (size + 1)))

    for i in range(1, size + 1):
        print(f"{i:2} |", end=" ")
        for j in range(1, size + 1):
            print(f"{i * j:4}", end=" ")
        print()

if __name__ == "__main__":
    print_multiplication_table()
